Author: Thomas Goirand <zigo@debian.org>
Forwarded: no
Last-Update: 2016-07-15

--- barbican-3.0.0~b2.orig/etc/barbican/barbican-api-paste.ini
+++ barbican-3.0.0~b2/etc/barbican/barbican-api-paste.ini
@@ -46,6 +46,13 @@ audit_map_file = /etc/barbican/api_audit
 
 [filter:authtoken]
 paste.filter_factory = keystonemiddleware.auth_token:filter_factory
+auth_host = 127.0.0.1
+auth_port = 5000
+auth_protocol = http
+admin_tenant_name = service
+admin_user = barbican
+admin_password = orange
+auth_version = v2.0
 
 [filter:profile]
 use = egg:repoze.profile
